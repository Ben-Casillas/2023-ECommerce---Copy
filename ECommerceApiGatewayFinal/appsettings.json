{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Spring": {
    "Application": {
      "Name": "ECommerceAPIGatewayOcelot"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl":"http://ECommerceEurekaRegistry:8761/eureka/",      
